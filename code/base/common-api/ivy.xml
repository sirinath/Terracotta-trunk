<?xml version="1.0" encoding="ISO-8859-1"?>
<ivy-module version="2.0">
  <info organisation="org.terracotta" module="common-api"/>
  <dependencies defaultconf="default">
    <dependency name="commons-cli" rev="1.1" org="commons-cli"/>
    <dependency name="commons-lang" rev="2.0" org="commons-lang"/>    
    <dependency name="commons-io" rev="1.2" org="commons-io"/>    
    <dependency name="concurrent" rev="1.3.4" org="concurrent"/>    
    <dependency name="jmxremote" rev="1.0.1_04" org="javax.management"/>
    <dependency name="knopflerfish-tc" rev="2.0.1" org="knopflerfish-tc"/>
    <dependency name="commons-httpclient" rev="3.1" org="commons-httpclient"/>
    <dependency name="log4j" rev="1.2.9" org="log4j"/>  
    <dependency name="xmlbeans-jsr173-api" rev="1.0" org="org.apache.xmlbeans"/>
    <dependency name="xmlbeans" rev="2.4.0" org="org.apache.xmlbeans">
      <exclude module="stax-api" org="stax" />
    </dependency>
    <dependency name="xmlbeans-xpath" rev="2.4.0" org="org.apache.xmlbeans">
      <exclude module="saxon" org="net.sf.saxon" />
    </dependency>
  </dependencies>
