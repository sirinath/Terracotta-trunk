<tc:tc-config xmlns:tc="http://www.terracotta.org/config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.terracotta.org/config ..\..\..\common\src.resources\com\tc\config\schema\terracotta-2.2.xsd">

	<system>
		<configuration-model>development</configuration-model>
	</system>

	<clients>
		<logs>%d/client-logs-%h</logs>
		<modules>
			<module name="clustered-rife-1.6.0" version="1.0.0" />
		</modules>
	</clients>

	<servers>
		<server name="localhost" />
	</servers>

	<application>
		<dso>
			<instrumented-classes>
				<include>
					<class-expression>com.tctest.*..*</class-expression>
				</include>
			</instrumented-classes>
			<locks>
				<autolock>
					<method-expression>* com.tctest.*..*.*(..)</method-expression>
					<lock-level>write</lock-level>
				</autolock>
			</locks>
		</dso>
	</application>

</tc:tc-config>
