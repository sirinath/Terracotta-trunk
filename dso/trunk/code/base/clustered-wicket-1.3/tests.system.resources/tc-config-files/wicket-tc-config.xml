<tc:tc-config 
    xmlns:tc="http://www.terracotta.org/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.terracotta.org/config ..\..\..\common\src.resources\com\tc\config\schema\terracotta-4.xsd">

  <system>
      <configuration-model>development</configuration-model>
  </system>

  <clients>
    <logs>terracotta/client-logs</logs>
    <dso>
      <debugging>
        <runtime-logging>
          <non-portable-dump>true</non-portable-dump>
        </runtime-logging>
      </debugging>
    </dso>
  </clients>

  <servers>
    <server name="localhost" />
  </servers>

  <application>
    <dso>
      <web-applications>
        <web-application>wicket-test</web-application>
      </web-applications>
      
      <instrumented-classes></instrumented-classes>
    </dso>
  </application>

</tc:tc-config>
